{
  "name": "@nexus-saude/nlp-service",
  "version": "1.0.0",
  "description": "Clinical Notes NLP Service - TypeScript conversion of Python service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t nexus-nlp-service .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f nlp-service",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.22.0",
    "@types/redis": "^4.0.11",
    "axios": "^1.6.0",
    "bull": "^4.12.2",
    "compromise": "^14.14.4",
    "compromise-sentences": "^0.3.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "fastify": "^4.29.1",
    "franc": "^6.1.0",
    "fuse.js": "^7.0.0",
    "html-to-text": "^9.0.5",
    "ioredis": "^5.7.0",
    "keyword-extractor": "^0.0.28",
    "kuromoji": "^0.1.2",
    "mammoth": "^1.6.0",
    "natural": "^6.12.0",
    "node-nlp": "^4.27.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "^8.21.0",
    "pino-pretty": "^10.2.3",
    "pos": "^0.4.2",
    "redis": "^4.7.1",
    "sentiment": "^5.0.2",
    "stemmer": "^2.0.1",
    "stopword": "^2.0.8",
    "uuid": "^9.0.1",
    "vader-sentiment": "^1.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/html-to-text": "^9.0.4",
    "@types/natural": "^5.1.3",
    "@types/node": "^20.19.17",
    "@types/pdf-parse": "^1.1.4",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "tsx": "^4.1.4",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nlp",
    "clinical",
    "medical",
    "text-processing",
    "entity-extraction",
    "document-classification",
    "summarization",
    "healthcare",
    "fastify",
    "typescript"
  ]
}
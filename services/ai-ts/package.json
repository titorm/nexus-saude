{
    "name": "@nexus-saude/ai-assistant",
    "version": "1.0.0",
    "description": "Medical AI Assistant Service - TypeScript conversion of Python service",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsx watch src/index.ts",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "docker:build": "docker build -t nexus-ai-assistant .",
        "docker:run": "docker run -p 8002:8002 nexus-ai-assistant"
    },
    "dependencies": {
        "fastify": "^4.24.3",
        "@fastify/cors": "^8.4.0",
        "@fastify/helmet": "^11.1.1",
        "@fastify/rate-limit": "^9.0.1",
        "@fastify/swagger": "^8.12.0",
        "@fastify/swagger-ui": "^2.0.0",
        "pino": "^8.16.2",
        "pino-pretty": "^10.2.3",
        "zod": "^3.22.4",
        "dotenv": "^16.3.1",
        "pg": "^8.11.3",
        "redis": "^4.6.10",
        "openai": "^4.20.1",
        "@tensorflow/tfjs-node": "^4.13.0",
        "natural": "^6.8.0",
        "compromise": "^14.10.0",
        "similarity": "^1.2.1",
        "axios": "^1.6.0",
        "uuid": "^9.0.1",
        "date-fns": "^2.30.0",
        "bull": "^4.12.2",
        "ioredis": "^5.3.2",
        "ml-matrix": "^6.10.7",
        "fuse.js": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.9.0",
        "@types/pg": "^8.10.7",
        "@types/uuid": "^9.0.6",
        "@types/natural": "^5.1.3",
        "typescript": "^5.2.2",
        "tsx": "^4.1.4",
        "vitest": "^0.34.6",
        "eslint": "^8.53.0",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "medical",
        "ai",
        "assistant",
        "healthcare",
        "nlp",
        "diagnosis",
        "recommendations",
        "fastify",
        "typescript"
    ]
}